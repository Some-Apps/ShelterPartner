{
  "hosting": {
    "source": ".",
    "site": "shelterpartner",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "redirects": [
      {
        "source": "/**",
        "destination": "https://app.shelterpartner.org",
        "type": 301
      }
    ]
  }
}